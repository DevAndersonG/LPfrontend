programa {
  funcao inicio() {
    real Num1 ,Num2 , Num3 ,media

    escreva("Digite o 1 Numero : ")
    leia(Num1)
    escreva("\nDigite o 2 Numero : ")
    leia(Num2)
    escreva("\nDigite o 3 Numero : ")
    leia(Num3)

    media = (Num1+Num2+ Num3)/3

    escreva("\n A media e ", media,"\n\n")
    
  }
}
